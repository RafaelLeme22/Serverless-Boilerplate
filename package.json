{
  "name": "serverless-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "db:reset": "npx sequelize db:drop; npx sequelize db:create; npx sequelize db:migrate; npx sequelize db:seed:all",
    "db:drop": "npx sequelize db:drop;",
    "db:create": "npx sequelize db:create;",
    "db:migrate": "npx sequelize db:migrate;",
    "db:seed": "npx sequelize db:seed:all;",
    "test:dev": "export NODE_ENV=dev && jest --config ./jest.config.ts --forceExit --coverage",
    "test:test": "export NODE_ENV=test && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && jest --testTimeout=10000 --maxWorkers=1",
    "dev": "export NODE_ENV=dev && serverless offline",
    "test": "export NODE_ENV=test && serverless offline",
    "lint": "eslint -c './.eslintrc' --fix './**/*.ts'"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.12.5"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.16.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.1",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "aws-lambda": "^1.0.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "serverless": "^2.69.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-typescript": "^2.1.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.10",
    "ts-node": "^10.2.1",
    "typescript": "4.0.5"
  }
}
